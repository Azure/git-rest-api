      "get": {
        "responses": {
          "200": {
            "description": ""
          }
        },
        "produces": [
          "application/json"
        ],
        "consumes": [
          "application/json"
        ]
      }
    },
    "/health/alive": {
      "get": {
        "responses": {
          "200": {
            "description": ""
          }
        },
        "produces": [
          "application/json"
        ],
        "consumes": [
          "application/json"
        ]
      }
