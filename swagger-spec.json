  "definitions": {
    "GitCommit": {
      "type": "object",
      "properties": {
        "sha": {
          "type": "string"
        }
      },
      "required": [
        "sha"
      ]
    },
    "GitBranch": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "commit": {
          "$ref": "#/definitions/GitCommit"
        }
      },
      "required": [
        "name",
        "commit"
      ]
    }
  }
